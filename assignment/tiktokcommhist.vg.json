{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "tiktokdata.csv"},
    "title": "Density Plot of Average Comments on TikTok",
    "mark": "area",
    "encoding": {
      "x": {
        "field": "avgcomments",
        "type": "quantitative",
        "axis": {"title": "Average Comments"},
        "bin": {"maxbins": 50}
      },
      "y": {
        "aggregate": "count",
        "type": "quantitative",
        "axis": {"title": "Number of Channels"}
      }
    }
  }
  